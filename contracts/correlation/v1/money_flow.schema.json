{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "flow_id",
    "version",
    "event_ids",
    "link_ids",
    "created_at"
  ],
  "properties": {
    "flow_id": {
      "type": "string",
      "minLength": 1
    },
    "version": {
      "type": "string",
      "pattern": "^v[0-9]+$"
    },
    "event_ids": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "link_ids": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}